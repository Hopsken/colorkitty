@require '~@/styles'

.wrapper
  min-height: calc(100vh - 72px)
  margin: qu(17.5) auto

  @media screen and (min-width: qu(280))
    max-width: qu(280)

  @media screen and (max-width: 1120px)
    width: qu(210)

  @media screen and (max-width: 900px)
    width: qu(140)

  @media screen and (max-width: 600px)
    margin-top: qu(5)
    width: qu(65)
